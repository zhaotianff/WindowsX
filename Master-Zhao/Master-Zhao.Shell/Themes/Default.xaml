<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Default silver-->
    <SolidColorBrush x:Key="AccentBaseColor" Color="Silver"/>
    <SolidColorBrush x:Key="AccentBaseColorTran" Color="Silver" Opacity=".2"/>
    <SolidColorBrush x:Key="AccentBaseColorShadow" Color="Silver" Opacity=".4"/>
    <SolidColorBrush x:Key="WhiteColor" Color="White"/>
    <SolidColorBrush x:Key="BlackColor" Color="Black"/>
    <SolidColorBrush x:Key="ButtonBorderColor" Color="#FF737373"/>
    <SolidColorBrush x:Key="ButtonDisabledBackgroundColor" Color="#FFF4F4F4"/>
    <SolidColorBrush x:Key="ButtonDisabledBorderColor" Color="#FFADB2B5"/>
    <SolidColorBrush x:Key="ButtonDisabledForegroundColor" Color="#FF838383"/>
    <SolidColorBrush x:Key="Items.Selected.Foreground" Color="Black"></SolidColorBrush>

    <Color x:Key="BorderColor">#FF737373</Color>
    <Color x:Key="BorderColorTran" A="120" R="115" G="115" B="115"></Color>
</ResourceDictionary>